                        62     KEYBOARD: ;takes the key pressed from the keyboard and puts it to A
0048 7580FF             63             mov     P0, #0ffh       ;makes P0 input
                        64     K1:
004B 759000             65             mov     P1, #0  ;ground all rows
004E E580               66             mov     A, P0
0050 547F               67             anl     A, #01111111B
0052 B47FF6             68             cjne    A, #01111111B, K1
                        69     K2:
0055 1137               70             acall   DELAY
0057 E580               71             mov     A, P0
0059 547F               72             anl     A, #01111111B
005B B47F02             73             cjne    A, #01111111B, KB_OVER
005E 80F5               74             sjmp    K2
                        75     KB_OVER:
0060 1137               76             acall DELAY
0062 E580               77             mov     A, P0
0064 547F               78             anl     A, #01111111B
0066 B47F02             79             cjne    A, #01111111B, KB_OVER1
0069 80EA               80             sjmp    K2
