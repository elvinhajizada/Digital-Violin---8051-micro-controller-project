                        14     CONFIGURE_LCD:  ;THIS SUBROUTINE SENDS THE INITIALIZATION COMMANDS TO THE LCD
0008 7438               15             mov a,#38H      ;TWO LINES, 5X7 MATRIX
000A 111D               16             acall SEND_COMMAND
000C 740F               17             mov a,#0FH      ;DISPLAY ON, CURSOR BLINKING
000E 111D               18             acall SEND_COMMAND
0010 7406               19             mov a,#06H      ;INCREMENT CURSOR (SHIFT CURSOR TO RIGHT)
0012 111D               20             acall SEND_COMMAND
0014 7401               21             mov a,#01H      ;CLEAR DISPLAY SCREEN
0016 111D               22             acall SEND_COMMAND
0018 7480               23             mov a,#80H      ;FORCE CURSOR TO BEGINNING OF THE FIRST LINE
001A 111D               24             acall SEND_COMMAND
001C 22                 25             ret
                        26
                        27
                        28
                        29     SEND_COMMAND:
001D F5A0               30             mov p2,a                ;THE COMMAND IS STORED IN A, SEND IT TO LCD
001F C2B5               31             clr p3.5                ;RS=0 BEFORE SENDING COMMAND
0021 C2B6               32             clr p3.6                ;R/W=0 TO WRITE
0023 D2B7               33             setb p3.7       ;SEND A HIGH TO LOW SIGNAL TO ENABLE PIN
0025 1137               34             acall DELAY
0027 C2B7               35             clr p3.7
0029 22                 36             ret
                        37
                        38
                        39     SEND_DATA:
002A F5A0               40             mov p2,a                ;SEND THE DATA STORED IN A TO LCD
002C D2B5               41             setb p3.5       ;RS=1 BEFORE SENDING DATA
002E C2B6               42             clr p3.6                ;R/W=0 TO WRITE
0030 D2B7               43             setb p3.7       ;SEND A HIGH TO LOW SIGNAL TO ENABLE PIN
0032 1137               44             acall DELAY
0034 C2B7               45             clr p3.7
0036 22                 46             ret
                        47
                        48
                        49     DELAY:
0037 C000               50             push 0
0039 C001               51             push 1
003B 7832               52             mov r0,#50
                        53     DELAY_OUTER_LOOP:
003D 79FF               54             mov r1,#255
003F D9FE               55             djnz r1,$
0041 D8FA               56             djnz r0,DELAY_OUTER_LOOP
0043 D001               57             pop 1
0045 D000               58             pop 0
0047 22                 59             ret
                        60
